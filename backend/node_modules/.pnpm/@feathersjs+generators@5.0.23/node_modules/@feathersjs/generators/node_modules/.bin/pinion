#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/bin/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/bin/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules/@featherscloud/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/@featherscloud+pinion@0.5.3/node_modules:/home/punit/codes/psoc/coupon-aggregator/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/bin/pinion.js" "$@"
else
  exec node  "$basedir/../../../../../../@featherscloud+pinion@0.5.3/node_modules/@featherscloud/pinion/bin/pinion.js" "$@"
fi
